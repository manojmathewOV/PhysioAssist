{
  "name": "physioassist",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "webpack serve --mode development --config webpack.config.js",
    "build:web": "webpack --mode production --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:ios:release": "detox test --configuration ios.sim.release",
    "test:e2e:ios:iphone14": "detox test --configuration ios.iphone14.debug",
    "test:e2e:ios:ipad": "detox test --configuration ios.ipad.debug",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "build:e2e:ios:release": "detox build --configuration ios.sim.release",
    "download-models": "./scripts/download-models.sh",
    "postinstall": "./scripts/download-models.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "pod-install": "cd ios && pod install",
    "prepare": "husky install"
  },
